      <xs:element xmlns:q3="http://schemas.datacontract.org/2004/07/Core.Standard.Quantities.TemperatureQuantity" minOccurs="0" name="Temperature" nillable="true" type="q3:Temperature" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="LogicalCore" nillable="true" type="tns:LogicalCore" />
</xs:schema>