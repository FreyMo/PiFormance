<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.datacontract.org/2004/07/PiFormance.Services.CpuRelated" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/PiFormance.Services.CpuRelated" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="http://schemas.datacontract.org/2004/07/Core.Common.Quantities.FrequencyQuantity" />
  <xs:import namespace="http://schemas.datacontract.org/2004/07/Core.Common.Quantities.RatioQuantity" />
  <xs:import namespace="http://schemas.datacontract.org/2004/07/Core.Common.Quantities.TemperatureQuantity" />
  <xs:import namespace="http://schemas.datacontract.org/2004/07/Core.Common.Quantities.MemoryQuantity" />
  <xs:complexType name="CpuSample">
    <xs:sequence>
      <xs:element xmlns:q1="http://schemas.datacontract.org/2004/07/Core.Common.Quantities.FrequencyQuantity" minOccurs="0" name="ClockSpeed" nillable="true" type="q1:Frequency" />
      <xs:element minOccurs="0" name="Cores" nillable="true" type="tns:ArrayOfCore" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="CpuSample" nillable="true" type="tns:CpuSample" />
  <xs:complexType name="ArrayOfCore">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Core" nillable="true" type="tns:Core" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfCore" nillable="true" type="tns:ArrayOfCore" />
  <xs:complexType name="Core">
    <xs:sequence>
      <xs:element minOccurs="0" name="Id" type="xs:int" />
      <xs:element xmlns:q2="http://schemas.datacontract.org/2004/07/Core.Common.Quantities.RatioQuantity" minOccurs="0" name="Load" nillable="true" type="q2:Ratio" />
      <xs:element xmlns:q3="http://schemas.datacontract.org/2004/07/Core.Common.Quantities.TemperatureQuantity" minOccurs="0" name="Temperature" nillable="true" type="q3:Temperature" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="Core" nillable="true" type="tns:Core" />
  <xs:complexType name="RamSample">
    <xs:sequence>
      <xs:element xmlns:q4="http://schemas.datacontract.org/2004/07/Core.Common.Quantities.MemoryQuantity" minOccurs="0" name="AvailableMemory" nillable="true" type="q4:Memory" />
      <xs:element xmlns:q5="http://schemas.datacontract.org/2004/07/Core.Common.Quantities.MemoryQuantity" minOccurs="0" name="TotalMemory" nillable="true" type="q5:Memory" />
      <xs:element xmlns:q6="http://schemas.datacontract.org/2004/07/Core.Common.Quantities.RatioQuantity" minOccurs="0" name="Usage" nillable="true" type="q6:Ratio" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="RamSample" nillable="true" type="tns:RamSample" />
</xs:schema>